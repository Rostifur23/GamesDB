@model IEnumerable<DBGames.Models.Engine>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EngineName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WrittenIn)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EngineName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WrittenIn)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EngineId }) |
            @Html.ActionLink("Details", "Details", new { id=item.EngineId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EngineId })
        </td>
    </tr>
}

</table>
